{
	"name": "create-ts-config",
	"version": "1.0.0",
	"description": "create a complete typescript project with one command",
	"keywords": [
		"react",
		"typescript",
		"tsconfig"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Itsnotaka/create-ts-config"
	},
	"author": "Min Chun Fu <itsnotaka@gmail.com>",
	"bin": {
		"create-ts-config": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "ncc build ./index.ts -w -o dist/",
		"prerelease": "rimraf ./dist/",
		"release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
		"prepublish": "yarn release"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/async-retry": "^1.4.2",
		"@types/node": "^18.0.0",
		"@types/prompts": "^2.0.1",
		"@types/rimraf": "^3.0.0",
		"@types/tar": "^6.1.1",
		"@types/validate-npm-package-name": "^3.0.0",
		"@vercel/ncc": "^0.34.0",
		"async-retry": "^1.3.1",
		"chalk": "^5.0.1",
		"commander": "^9.3.0",
		"cpy": "^9.0.1",
		"cross-spawn": "^7.0.3",
		"got": "^12.1.0",
		"prompts": "^2.1.0",
		"rimraf": "^3.0.0",
		"tar": "^6.1.11",
		"typescript": "^4.7.4",
		"update-check": "^1.5.4",
		"validate-npm-package-name": "^4.0.0"
	},
	"engines": {
		"node": ">=12.22.0"
	}
}
